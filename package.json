{
  "name": "rust-server-site",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "install-all": "./install.sh",
    "build": "cd frontend && npm run build && cd ../bot && npm run build && cd ..",
    "start": "node bot/dist/index.js",
    "dev": "concurrently \"cd frontend && npm run dev\" \"cd bot && npm run dev\"",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:bot": "cd bot && npm run dev"
  },
  "devDependencies": {
    "concurrently": "^8.2.0"
  },
  "dependencies": {
    "next": "^13.5.9",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.4.0",
    "postcss": "^8.4.24",
    "autoprefixer": "^10.4.14",
    "gamedig": "^2.0.21",
    "axios": "^1.4.0"
  }
}
