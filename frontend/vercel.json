{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "package.json",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "^/(?!.*\\.).*$",
      "dest": "/index.html"
    },
    {
      "src": "^/api/(.*)",
      "dest": "/api/$1"
    }
  ],
  "env": {
    "VITE_API_URL": "@vite_api_url"
  }
}
